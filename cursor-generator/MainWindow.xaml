<Window x:Class="cursor_generator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cursor_generator"
        mc:Ignorable="d"
        Title="Cursor Generator" Height="135.227" Width="453.409">
    <Grid>
        <TextBox x:Name="sourceImageTextBox" HorizontalAlignment="Left" Height="23" Margin="11,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="324"/>
        <Button x:Name="selectSourceImageButton" Content="Select File" HorizontalAlignment="Left" Margin="347,32,0,0" VerticalAlignment="Top" Width="89" Height="23" Click="selectSourceImageButton_Click"/>
        <Button x:Name="saveCursorButton" Content="Save Cursor" HorizontalAlignment="Left" Margin="347,67,0,0" VerticalAlignment="Top" Width="89" Click="saveCursorButton_Click" Height="27"/>
        <Label x:Name="sourceFileLabel" Content="Source File" HorizontalAlignment="Left" Margin="8,11,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
